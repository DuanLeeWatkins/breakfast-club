<%- include('partials/header') -%>
<div class="container">
  <h2>Feed</h2>

  <p>Hello <%=user.userName%></p>
  <span>What's for breakfast?</span>

  <div class="container mx-auto">
    <% for( let i = 0; i < posts.length; i++ ) { %>
    <div class="mb-5" style="outline: 1px solid black">
      <img src="<%=posts[i].image%>" alt="<%=posts[i].name%>" />
      <p><%=posts[i].name%></p>
      <p>Created by:<%=posts[i].user%></p>
      <button>This should add likes to user object array (likedPhotos)</button>
    </div>
    <% } %>
  </div>
  <%- include('partials/footer') -%>
</div>
